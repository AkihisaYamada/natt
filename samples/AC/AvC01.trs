(THEORY (A mat_mult) (AC +))
(VAR x y z)
(RULES
	mat_mult(I,x) -> x
	mat_mult(x,I) -> x
	mat_mult(x,+(y,z)) -> +(mat_mult(x,y), mat_mult(x,z))
	+(x,0) -> x
	mat_pow(x,0) -> I
	mat_pow(x,+(y,y)) -> mat_pow(mat_mult(x,x),y)
	mat_pow(x,+(1,+(y,y))) -> mat_mult(x, mat_pow(x,+(y,y)))
	mat_mult(x,sprod(y,z)) -> mat_mult(sprod(y,x),z)
)
