(VAR x y z)
(RULES
	add'(y) -> pair(0,y)
	mul'(0) -> pair(0,y)
	add'(s(z)) -> u1(add'(z))
	mul'(0) -> pair(x,0)
	u1(pair(x,y)) -> pair(s(x),y)
	mul'(s(z)) -> u2(add'(z))
	add'(add(x,y)) -> pair(x,y)
	u2(pair(w,y)) -> u3(mul'(w),y)
	u3(pair(x,s(y)),y) -> pair(s(x),s(y))
	mul'(mul(x,y)) -> pair(x,y)
)
