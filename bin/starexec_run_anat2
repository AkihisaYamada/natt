#!/bin/sh
DIR=${0%/*}
PATH=$DIR:$PATH
STR='<strategy>
	<edg/>
	<order>
		<template name="anat2">
			<entry range="pos">
				<case arity="0">
					<var range="pos"/>
				</case>
				<args mode="sum">
					<max>
						<sum>
							<prod>
								<arg coord="0"/>
								<var range="bool"/>
							</prod>
							<var range="pos"/>
						</sum>
						<sum>
							<prod>
								<arg coord="1"/>
								<var range="pos"/>
							</prod>
							<prod>
								<arg coord="2"/>
								<var range="bool"/>
							</prod>
						</sum>
					</max>
				</args>
			</entry>
			<entry range="bool">
				<case arity="0">
					<var range="bool"/>
				</case>
				<args mode="prod">
					<max>
						<var range="bool"/>
						<arg coord="1"/>
					</max>
				</args>
			</entry>
			<entry range="pos">
				<case arity="0">
					<var range="pos"/>
				</case>
				<args mode="sum">
					<max>
						<sum>
							<prod>
								<arg coord="0"/>
								<var range="bool"/>
							</prod>
							<var range="pos"/>
						</sum>
						<sum>
							<prod>
								<arg coord="1"/>
								<var range="pos"/>
							</prod>
							<prod>
								<arg coord="2"/>
								<var range="bool"/>
							</prod>
						</sum>
					</max>
				</args>
			</entry>
		</template>
	</order>
</strategy>'
NaTT.exe -S:"$STR" "$1" 2> "$2/stderr.txt"
cat $2/stderr.txt
