#!/bin/sh
DIR=${0%/*}
PATH=$DIR:$PATH
STR='<strategy>
  <edg/>
  <order>
    <template name="max">
      <entry range="pos">
        <case arity="0">
          <var range="pos"/>
        </case>
        <case arity="1">
          <sum>
            <prod>
              <var range="bool"/>
              <arg coord="0"/>
            </prod>
            <var range="pos"/>
          </sum>
        </case>
        <args mode="max">
          <prod>
            <var range="bool"/>
            <sum>
              <arg coord="0"/>
              <var range="pos"/>
            </sum>
          </prod>
        </args>
      </entry>
    </template>
  </order>
</strategy>'
NaTT.exe -S:"$STR" "$1" 2> "$2/stderr.txt"
cat $2/stderr.txt
