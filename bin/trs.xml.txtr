<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<txtruct>
	<class name="comment">
		<match as="cmt">\(COMMENT\s([^)]*)\)\s*</match>
		<comment>
			<value-of select="cmt/group[1]"/>
		</comment>
	</class>
	<class name="symbol">
		<match as="s">([0-9A-Za-z_+\-*/.\\:=!?&lt;>\[\]']+)\s*</match>
		<value-of select="s/group[1]"/>
	</class>
	<class name="var">
		<read ref="symbol" as="sym"/>
		<element name="var">
			<value-of select="sym"/>
		</element>
	</class>
	<class name="syms">
		<option>
			<match>\(VAR(\s+|(?=[\(\)\[\]\{\}]))</match>
			<read ref="var" maxOccurs="unbounded"/>
			<match>\)\s*</match>
		</option>
		<option>
			<match>\(THEORY(\s+|(?=[\(\)\[\]\{\}]))</match>
			<group minOccurs="0" maxOccurs="unbounded">
				<match as="th">\((AC|A|C)\s+</match>
				<group maxOccurs="unbounded">
					<read ref="symbol" as="s"/>
					<element name="sym">
						<attribute name="theory">
							<value-of select="th/group[1]"/>
						</attribute>
						<value-of select="s"/>
					</element>
				</group>
				<match>\)\s*</match>
			</group>
			<match>\)\s*</match>
		</option>
	</class>
	<class name="term">
		<read ref="symbol" as="s"/>
		<choice>
			<group>
				<match>\(\s*</match>
				<element name="app">
					<element name="sym">
						<value-of select="s"/>
					</element>
					<group minOccurs="0">
						<read ref="term"/>
						<group minOccurs="0" maxOccurs="unbounded">
							<match>\s*,\s*</match>
							<read ref="term"/>
						</group>
					</group>
					<match>\)\s*</match>
				</element>
			</group>
			<group>
				<element name="sym">
					<value-of select="s"/>
				</element>
			</group>
		</choice>
	</class>
	<class name="cond">
		<read ref="term" as="lhs"/>
		<match>\=\=\s*</match>
		<read ref="term" as="rhs"/>
		<element name="cond">
			<value-of select="lhs"/>
			<value-of select="rhs"/>
		</element>
	</class>
	<class name="conds">
		<read ref="cond"/>
		<group minOccurs="0" maxOccurs="unbounded">
			<match>,\s*</match>
			<read ref="cond"/>
		</group>
	</class>
	<class name="rule">
		<read ref="term" as="lhs"/>
		<match>->\s*</match>
		<read ref="term" as="rhs"/>
		<element name="rule">
			<value-of select="lhs"/>
			<value-of select="rhs"/>
			<group minOccurs="0">
				<match>\|\s+</match>
				<read ref="conds"/>
			</group>
		</element>
		<match>\s*</match>
	</class>
	<class name="rules">
		<match>\(RULES\s+</match>
		<read ref="rule" maxOccurs="unbounded"/>
		<match>\)\s*</match>
	</class>

	<element name="trs">
		<read ref="comment" minOccurs="0" maxOccurs="unbounded"/>
		<group minOccurs="0">
			<match as="prob">\(PROBLEM\s+([^)]*)\)\s*</match>
			<attribute name="problem">
				<value-of select="prob/group[1]"/>
			</attribute>
			<read ref="comment" minOccurs="0" maxOccurs="unbounded"/>
		</group>
		<group minOccurs="0">
			<match as="ct">\(CONDITIONTYPE\s+(.*)\)\s*</match>
			<attribute name="condition-type">
				<value-of select="ct/group[1]"/>
			</attribute>
			<read ref="comment" minOccurs="0" maxOccurs="unbounded"/>
		</group>
		<element name="syms">
			<read ref="syms" minOccurs="0" maxOccurs="unbounded"/>
		</element>
		<read ref="comment" minOccurs="0" maxOccurs="unbounded"/>
		<read ref="rules"/>
		<read ref="comment" minOccurs="0" maxOccurs="unbounded"/>
		<if test="prob/group[1] = 'INFEASIBILITY'">
			<element name="infeasibility">
				<element name="syms">
					<read ref="syms" minOccurs="0" maxOccurs="unbounded"/>
				</element>
				<match>\(CONDITION\s+</match>
				<read ref="conds"/>
			</element>
			<match>\)\s*</match>
			<read ref="comment" minOccurs="0" maxOccurs="unbounded"/>
		</if>
	</element>
</txtruct>