#!/bin/sh
DIR=${0%/*}
PATH=$DIR:$PATH
STR='<strategy>
  <edg/>
  <order>
    <template name="sum_sum_neg_max">
      <entry range="pos">
        <case arity="0">
          <var range="pos"/>
        </case>
        <max>
          <sum>
            <args mode="sum">
              <sum>
                <prod>
                  <var range="bool"/>
                  <arg coord="0"/>
                </prod>
                <prod>
                  <var range="bool"/>
                  <arg coord="1"/>
                </prod>
              </sum>
            </args>
            <var range="full"/>
          </sum>
          <const>0</const>
        </max>
      </entry>
      <entry range="neg">
        <case arity="0">
          <var range="neg"/>
        </case>
        <case arity="1">
          <sum>
            <prod>
              <var range="bool"/>
              <arg coord="1"/>
            </prod>
            <var range="neg"/>
          </sum>
        </case>
        <args mode="max">
          <prod>
            <var range="bool"/>
            <sum>
              <arg coord="1"/>
              <var range="neg"/>
            </sum>
          </prod>
        </args>
      </entry>
    </template>
  </order>
</strategy>'
NaTT.exe -S:"$STR" "$1" 2> "$2/stderr.txt"
cat $2/stderr.txt
