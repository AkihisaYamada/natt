#!/bin/sh
DIR=${0%/*}
PATH=$DIR:$PATH
STR='<strategy>
  <edg/>
  <order>
    <template name="max_max_full_max">
      <entry range="pos">
        <case arity="0">
          <var range="pos"/>
        </case>
        <case arity="1">
          <max>
            <sum>
              <prod>
                <var range="bool"/>
                <arg coord="0"/>
              </prod>
              <var range="full"/>
            </sum>
            <prod>
              <var range="bool"/>
              <sum>
                <arg coord="1"/>
                <var range="full"/>
              </sum>
            </prod>
            <const>0</const>
          </max>
        </case>
        <max>
          <args mode="max">
            <max>
              <prod>
                <var range="bool"/>
                <sum>
                  <arg coord="0"/>
                  <var range="full"/>
                </sum>
              </prod>
              <prod>
                <var range="bool"/>
                <sum>
                  <arg coord="1"/>
                  <var range="full"/>
                </sum>
              </prod>
            </max>
          </args>
          <const>0</const>
        </max>
      </entry>
      <entry range="full">
        <case arity="0">
          <var range="full"/>
        </case>
        <case arity="1">
          <sum>
            <prod>
              <var range="bool"/>
              <arg coord="1"/>
            </prod>
            <var range="full"/>
          </sum>
        </case>
        <args mode="max">
          <prod>
            <var range="bool"/>
            <sum>
              <arg coord="1"/>
              <var range="full"/>
            </sum>
          </prod>
        </args>
      </entry>
    </template>
  </order>
</strategy>'
NaTT.exe -S:"$STR" "$1" 2> "$2/stderr.txt"
cat $2/stderr.txt
