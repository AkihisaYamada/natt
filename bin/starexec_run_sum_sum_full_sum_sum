#!/bin/sh
DIR=${0%/*}
PATH=$DIR:$PATH
STR='<strategy>
  <edg/>
  <order>
    <template name="sum_sum_full_sum_sum">
      <entry range="pos">
        <case arity="0">
          <var range="pos"/>
        </case>
        <max>
          <sum>
            <args mode="sum">
              <sum>
                <prod>
                  <var range="bool"/>
                  <arg coord="0"/>
                </prod>
                <prod>
                  <var range="bool"/>
                  <arg coord="1"/>
                </prod>
              </sum>
            </args>
            <var range="full"/>
          </sum>
          <const>0</const>
        </max>
      </entry>
      <entry range="full">
        <sum>
          <args>
            <sum>
              <prod>
                <var range="bool"/>
                <arg coord="0"/>
              </prod>
              <prod>
                <var range="bool"/>
                <arg coord="1"/>
              </prod>
            </sum>
          </args>
          <var range="full"/>
        </sum>
      </entry>
    </template>
  </order>
</strategy>'
NaTT.exe -S:"$STR" "$1" 2> "$2/stderr.txt"
cat $2/stderr.txt
