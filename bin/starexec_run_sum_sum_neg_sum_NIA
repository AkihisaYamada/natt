#!/bin/sh
DIR=${0%/*}
PATH=$DIR:$PATH
STR='<strategy>
  <edg/>
  <order>
    <smt linear="false"><z3/></smt>
    <template name="sum_sum_neg_sum_NIA">
      <entry>
        <case arity="0">
          <var range="pos"/>
        </case>
        <max>
          <sum>
            <args>
              <sum>
                <prod>
                  <var range="pos"/>
                  <arg coord="0"/>
                </prod>
                <prod>
                  <var range="pos"/>
                  <arg coord="1"/>
                </prod>
              </sum>
            </args>
            <var range="full"/>
          </sum>
          <const>0</const>
        </max>
      </entry>
      <entry range="neg">
        <sum>
          <args>
            <prod>
              <var range="pos"/>
              <arg coord="1"/>
            </prod>
          </args>
          <var range="neg"/>
        </sum>
      </entry>
    </template>
  </order>
</strategy>'
NaTT.exe -S:"$STR" "$1" 2> "$2/stderr.txt"
cat $2/stderr.txt
